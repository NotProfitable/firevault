  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "dev-electron": "concurrently -k \"next dev\" \"electron .\"",
    "build-electron": "next build && next export && electron-builder --dir"
  },
  "main": "main/index.js",
  "build": {
    "asar": true,
    "files": [
      "main",
      "out"
    ]
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
